	text-transform: uppercase; /* Convertir texto a mayúsculas */
}

/* Estilo del Botón al Pasar el Ratón (Hover) */
QPushButton:hover {
    color: #445d5e;
    font-weight: bold;
    font-size: 15px;
    border-bottom: 2px solid #445d5e;
}

/* Estilo del Botón Deshabilitado */
QPushButton:disabled {
    color: #54BFC9;
    border-bottom: 3px solid #54BFC9;
}

/* Estilo del Botón Activo (Pestaña Activa) */
QPushButton:checked, QPushButton:focus {
    color: #000000; /* Puedes ajustar el color para que destaque más */
    font-weight: bold;
    font-size: 15px;
    border-bottom: 3px solid #3f5758; /* Color que indique claramente la pestaña activa */
}

